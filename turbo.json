{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": ["REACT_APP_BASE_URL", "MONGO_URI", "PORT", "SECRET_JWT", "G_CLIENT_ID", "G_CLIENT_SECRET", "G_REFRESH_TOKEN", "ADMIN_EMAIL"]
}
